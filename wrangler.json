{
  "name": "zona-natural",
  "compatibility_date": "2024-11-14",
  "compatibility_flags": ["nodejs_compat"],

  "pages_build_output_dir": ".vercel/output/static",

  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "zona-natural-db",
      "database_id": "REEMPLAZA_CON_TU_ID"
    }
  ],
  "r2_buckets": [
    {
      "binding": "R2_BUCKET",
      "bucket_name": "zona-natural-uploads"
    }
  ],
  "vars": {
    "PUBLIC_R2_BASE_URL": "https://TU-DOMINIO-O-CDN/zone"
  }
}
