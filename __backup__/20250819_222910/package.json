{
  "name": "zona-natural",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint .",
    "db:studio": "drizzle-kit studio",
    "format": "prettier -w .",
    "build": "next build && npx @cloudflare/next-on-pages@1",
    "db:migrate": "prisma migrate dev --name init",
    "db:push": "drizzle-kit push",
    "dev": "next dev",
    "db:generate": "drizzle-kit generate",
    "preview": "wrangler pages dev .vercel/output/static --compatibility-date=2025-08-18",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "node prisma/seed.js",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "6.14.0",
    "bcryptjs": "3.0.2",
    "drizzle-orm": "^0.33.0",
    "jose": "6.0.12",
    "next": "14.2.4",
    "prisma": "6.14.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.0",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.23",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.24.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "eslint-plugin-next-on-pages": "^1.10.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "wrangler": "^3.60.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
